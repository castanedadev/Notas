CREATE PROCEDURE NGMRep

@materia int,@grado int,@seccion 
int

as

BEGIN

SELECT     
dbo.Maestros.Nombre, dbo.MATERIA.Materia, dbo.GRADO.Grado, dbo.SECCION.Seccion
FROM            dbo.NGM INNER JOIN
                         dbo.MATERIA ON dbo.NGM.Materia = dbo.MATERIA.IdMateria INNER JOIN
                         dbo.GRADO ON dbo.NGM.Grado = dbo.GRADO.IdGrado INNER JOIN
                         dbo.SECCION ON dbo.NGM.Seccion = dbo.SECCION.IdSeccion INNER JOIN
                         dbo.Maestros ON dbo.NGM.Maestro = dbo.Maestros.IdMaestro
						 WHERE Materia.IdMateria=@materia AND SECCION.IdSeccion=@seccion and Grado.IdGrado=@grado
END
